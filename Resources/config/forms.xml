<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="qcm_admin.form.type.user_session_complete.class">Qcm\Bundle\AdminBundle\Form\Type\UserSessionCompleteFormType</parameter>
    </parameters>

    <services>
        <service id="qcm_admin.form.type.user_session_complete" class="%qcm_admin.form.type.user_session_complete.class%">
            <argument>%qcm_core.model.user_session_configuration.class%</argument>
            <argument>%qcm_core.validation_group.user_session%</argument>
            <tag name="form.type" alias="qcm_admin_user_session_complete" />
        </service>
    </services>
</container>
